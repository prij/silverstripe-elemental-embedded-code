{
    "name": "dynamic/silverstripe-elemental-embedded-code",
    "description": "Embed code like iFrames or Javascript on a page.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content",
        "blocks",
        "embedded",
        "iframe",
        "javascript"
    ],
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/dynamic"
        }
    ],
    "require": {
        "php": "^8.1",
        "dnadesign/silverstripe-elemental": "^6",
        "silverstripe/framework": "^6"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\Embedded\\": "src/",
            "Dynamic\\Elements\\Embedded\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "silverstripe/vendor-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    }
}
